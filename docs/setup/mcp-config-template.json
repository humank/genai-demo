{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/yikaikao/git"
      ],
      "env": {},
      "description": "File system operations - read, write, search files and directories"
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@cyanheads/git-mcp-server"
      ],
      "env": {
        "GIT_REPOSITORY_PATH": "/Users/yikaikao/git"
      },
      "description": "Git version control operations"
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {},
      "disabled": false,
      "autoApprove": [],
      "description": "HTTP requests and web content fetching"
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "your-brave-api-key-here"
      },
      "disabled": true,
      "description": "Web search using Brave Search API"
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres"
      ],
      "env": {
        "POSTGRES_CONNECTION_STRING": "postgresql://user:password@localhost:5432/dbname"
      },
      "disabled": true,
      "description": "PostgreSQL database operations"
    },
    "sqlite": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sqlite",
        "/path/to/database.db"
      ],
      "env": {},
      "disabled": true,
      "description": "SQLite database operations"
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "your-github-token-here"
      },
      "disabled": true,
      "description": "GitHub repository and issue management"
    },
    "slack": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-slack"
      ],
      "env": {
        "SLACK_BOT_TOKEN": "your-slack-bot-token-here"
      },
      "disabled": true,
      "description": "Slack messaging and channel management"
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "env": {},
      "disabled": true,
      "description": "Web scraping and browser automation"
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {},
      "disabled": true,
      "description": "Persistent memory and knowledge storage"
    },
    "everything": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ],
      "env": {},
      "disabled": true,
      "description": "File search using Everything (Windows) or similar tools"
    },
    "aws": {
      "command": "python",
      "args": [
        "-m",
        "awslabs.core_mcp_server"
      ],
      "env": {
        "AWS_PROFILE": "default",
        "AWS_REGION": "us-east-1"
      },
      "disabled": true,
      "description": "AWS services integration"
    }
  }
}